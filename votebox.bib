@TECHREPORT{bellare97forward,
  author = {M. Bellare and B. Yee},
  title = {Forward integrity for secure audit logs},
  institution = {UC at San Diego, Dept. of Computer Science and Engineering},
  year = {1997},
  month = nov,
  annote = {Bellare uses an evolving secret key to MAC each subsequent log entry.
	K_i is derived from K_{i-1} using a one-way function, at which point
	K_{i-1} is destroyed. A trusted third party holds K_0, the base key,
	which can be used to validate that every log entry is authentic and
	present (ie. none has been deleted). This provides "forward integrity"
	-- the property that an attacker cannot erase, insert, or alter events
	in the past.},
