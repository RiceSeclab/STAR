@TECHREPORT{benaloh91efficient,
  author = {Josh Benaloh and Michael de Mare},
  title = {Efficient Broadcast time-stamping},
  institution = {Clarkson University Department of Mathematics and Computer Science},
  year = {1991},
  number = {1},
  month = aug,
  abstract = {Benaloh and de Mare provide forward and backward timestamping ("happened
	before" and "happened after" semantics for any event) without the
	need for a central timestamping authority or widespread storage of
	documents as in Haber and Stornetta 91. Their system relies only
	on an "authenticated broadcast" primitive.
	
	They do mention ({\S}3.1) the "Remember Everything" model, which they
	claim is too space-inefficient for any practical use (not so in our
	application, or really in any modern system). {\S}3.2 refines this
	by replacing entire documents with hashes; each participant simply
	remembers the current global authenticator. This requires a lot of
	cooperation in order to verify anything. The Benaloh/de Mare solution,
	{\S}3.3, is organized into "rounds": you must only store other documents
	if they were submitted in the same round as a document of yours.
	Everyone remembers the hash of every round. To verify your timestamp,
	you must submit all the documents (including yours!) that were posted
	that round; the hash of the set must match the round's hash. (This
	is identical to "everyone remembers everything" if you post documents
	frequently!)},
  bdsk-url-1 = {citeseer.ist.psu.edu/benaloh91efficient.html},
  date-added = {2006-10-29 21:41:57 -0600},
  date-modified = {2007-02-03 18:16:09 -0800},
  keywords = {timestamping},
  local-url = {file://localhost/Users/dsandler/Documents/Research/Auditorium/papers/oakland/papers/benaloh91efficient.pdf},
  text = {Josh Benaloh and Michael de Mare. Efficient Broadcast time-stamping.
	Technical Report 1, Clarkson University Department of Mathematics
	and Computer Science, Aug. 1991.},
  url = {citeseer.ist.psu.edu/benaloh91efficient.html}
}

